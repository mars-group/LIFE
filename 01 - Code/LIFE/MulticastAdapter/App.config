<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key ="UseAllInterfaces" value="true"/>

    <!-- Specifiy all network interfaces that should be used for multicast here. In case u don’t want to use all interfaces.-->
    <add key ="Interface01Name" value="Ethernet0"/>
    <add key ="Interface01IPv4" value="10.0.0.6"/>

  </appSettings>
  
  
  
</configuration>